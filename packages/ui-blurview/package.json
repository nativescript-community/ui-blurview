{
    "name": "@nativescript-community/ui-blurview",
    "version": "1.2.2",
    "description": "A blurview plugin",
    "main": "index",
    "typings": "index.d.ts",
    "sideEffects": false,
    "nativescript": {
        "platforms": {
            "android": "3.0.0",
            "ios": "3.0.0"
        }
    },
    "scripts": {
        "build": "npm run tsc",
        "build.all": "npm run build",
        "build.angular": "ng-packagr -p ../../src/ui-blurview/angular/ng-package.json -c ../../src/ui-blurview/angular/tsconfig.json",
        "readme": "readme generate -c ../../tools/readme/blueprint.json",
        "tsc": "cpy '**/*.d.ts' '../../packages/ui-blurview' --parents --cwd=../../src/ui-blurview && tsc -skipLibCheck -d",
        "clean": "rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin.guillon@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/nativescript-community/ui-blurview/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/nativescript-community/ui-blurview",
    "readmeFilename": "README.md",
    "bootstrapper": "nativescript-plugin-seed"
}
